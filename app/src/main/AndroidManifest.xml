<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mobileapp.learnkotlin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mobileapp.learnkotlin.codelabs.androidtrivia.AndroidTriviaActivity"></activity>
        <activity android:name="com.mobileapp.learnkotlin.codelabs.constraintlayout.ColorMyViewsAppActivity" />
        <activity android:name=".LauncherActivity"
                    android:theme="@style/Theme.MaterialComponents.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mobileapp.learnkotlin.codelabs.diceapp.DiceAppActivity" />
        <activity android:name=".codelabs.guesstheword.GuessTheWordActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>